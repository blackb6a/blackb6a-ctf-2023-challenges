{
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#dc9",
        "editorBracketHighlight.foreground2": "#ffbe33",
        "editorBracketHighlight.foreground3": "#dc9",
        "editorBracketHighlight.foreground4": "#ffbe33",
        "editor.selectionBackground": "#ffffff80",
        "minimap.selectionHighlight": "#ff0000"
    },
    "editor.tokenColorCustomizations": {
        "types": "#d1491b",
        "numbers": "#bd777f",
        "strings": "#8f1313",
        "comments": "#753128",
        "keywords": "#9c2700",
        "functions": "#f76c3e",
        "variables": "#dc9",
        "textMateRules": [
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#ffbe33"
                }
            },
            {
                "scope": [
                    "source.python",
                    "source.raccoon",
                    "keyword.operator"
                ],
                "settings": {
                    "foreground": "#8f4f56"
                }
            },
            {
                "scope": "support.variable",
                "settings": {
                    "foreground": "#dc9"
                }
            },
            {
                "scope": [
                    "keyword.operator.logical.python",
                    "keyword.operator.logical.raccoon",
                    "entity.name.tag",
                    "storage.modifier",
                    "constant.language",
                    "variable.language"
                ],
                "settings": {
                    "foreground": "#9c2700"
                }
            },
            {
                "scope": [
                    "constant.character.escape",
                    "constant.character.unicode"
                ],
                "settings": {
                    "foreground": "#d68556"
                }
            }
        ]
    }
}