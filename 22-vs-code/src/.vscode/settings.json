{
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#ffffff80",
        "minimap.background": "#202020"
    },
    "editor.tokenColorCustomizations": {
        "comments": "#315528", //first 2 comments
        "functions": "#6cf73e", //enumerate
        "keywords": "#279c00", //def,lambda,r ['26', '27', '28', '29', '2a', '2b', '2c'] 9c 0
        "numbers": "#f5e6a0", // OVERWRITTEN BY constant.numeric
        "strings": "#138f13", //M in encrypted flag
        "types": "#49d11b", //49d1 ['16', '17', '18', '19', '1a', '1b']
        "variables": "#ccdd99", //! flags 
        "textMateRules": [
            {
                "scope": [
                    "source.python"
                ],
                "settings": {
                    "foreground": "#5a8f5a" //first line
                }
            },
            {
                "scope": "keyword.control",
                "settings": {
                    "foreground": "#beff33" //import with
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#ffffbe" //*<=
                }
            },
            {
                "scope": "keyword.operator.logical.python",
                "settings": {
                    "foreground": "#00ff00" //or
                }
            },
            {
                "scope": [
                    "entity.name.tag",
                    "constant.language",
                    "variable.language"
                ],
                "settings": {
                    "foreground": "#9cff27" //9cff['25', '26', '27', '28', '29', '2a', '2c']
                }
            },
            {
                "scope": [
                    "constant.character.escape",
                    "constant.character.unicode"
                ],
                "settings": {
                    "foreground": "#85d656" //\x62
                }
            },
            {
                "scope": [
                    "constant.character.set.regexp",
                    "string.regexp"
                ],
                "settings": {
                    "foreground": "#defaca" //A-Za-z0-9-_
                }
            }
        ]
    }
}